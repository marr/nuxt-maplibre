{
  "name": "nuxt-maplibre",
  "version": "1.0.1",
  "description": "A Nuxt module to use MapLibre",
  "homepage": "https://gugustinette.github.io/nuxt-maplibre",
  "repository": "https://github.com/gugustinette/nuxt-maplibre",
  "bugs": {
    "url": "https://github.com/gugustinette/nuxt-maplibre/issues"
  },
  "license": "Apache-2.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:host": "nuxi dev playground --host",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "@indoorequal/vue-maplibre-gl": "^8.2.2",
    "maplibre-gl": "^5.2.0",
    "maplibre-google-maps": "^1.1.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/eslint-config": "^1.2.0",
    "@nuxt/kit": "^3.16.0",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.16.0",
    "@nuxt/test-utils": "^3.17.2",
    "@nuxt/ui": "^2.21.1",
    "@types/node": "^22.13.10",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "caniuse-lite": "^1.0.30001703",
    "changelogen": "^0.6.1",
    "eslint": "^9.22.0",
    "nuxt": "^3.16.0",
    "vitepress": "^1.6.3",
    "vitest": "^3.0.8"
  }
}
